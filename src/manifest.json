{
  "name": "Boiteajeux Agricola Extension",
  "version": "0.1",
  "description": "Extends Agricola game at Boiteajeux for a better gaming experience",
  "permissions": ["declarativeContent", "http://www.boiteajeux.net/"],
  "background": {
    "scripts": ["background.js"],
